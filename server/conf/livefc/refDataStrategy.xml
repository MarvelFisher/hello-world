<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context.xsd
	http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans.xsd">

    <context:annotation-config />

    <bean id="strategyMapping" class="java.util.HashMap">
        <constructor-arg>
            <map>
                <entry key="IF" value-ref="ifList"/>
            </map>
        </constructor-arg>
    </bean>

    <bean id="ifList" class="java.util.ArrayList">
        <constructor-arg>
            <list>
                <ref bean="ifData"/>
                <ref bean="icData"/>
                <ref bean="ihData"/>
            </list>
        </constructor-arg>
    </bean>

    <bean id="ifData" class="com.cyanspring.common.staticdata.fu.MappingData">
        <property name="near1" value="IFC1"/>
        <property name="near2" value="IFC2"/>
        <property name="season1" value="IFC3"/>
        <property name="season2" value="IFC4"/>
        <property name="symbol" value="IF"/>
    </bean>

    <bean id="icData" class="com.cyanspring.common.staticdata.fu.MappingData">
        <property name="near1" value="ICC1"/>
        <property name="near2" value="ICC2"/>
        <property name="season1" value="ICC3"/>
        <property name="season2" value="ICC4"/>
        <property name="symbol" value="IC"/>
    </bean>

    <bean id="ihData" class="com.cyanspring.common.staticdata.fu.MappingData">
        <property name="near1" value="IHC1"/>
        <property name="near2" value="IHC2"/>
        <property name="season1" value="IHC3"/>
        <property name="season2" value="IHC4"/>
        <property name="symbol" value="IH"/>
    </bean>

</beans>
